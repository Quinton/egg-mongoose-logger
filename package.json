{
  "name": "egg-mongoose-logger",
  "version": "1.0.5",
  "description": "A helper plugin to hook egg logger library in mongoose.",
  "eggPlugin": {
    "name": "mongooseLogger",
    "optionalDependencies": [
      "egg-mongoose"
    ]
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "egg-mongoose": "^2.1.0"
  },
  "devDependencies": {
    "autod": "^2.8.0",
    "autod-egg": "^1.0.0",
    "egg": "^1.4.0",
    "egg-bin": "^3.4.0",
    "egg-ci": "^1.6.0",
    "egg-mock": "^3.7.0",
    "egg-mongoose": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-egg": "^4.2.0",
    "supertest": "^3.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "egg-bin pkgfiles --check && npm run lint && npm run cov",
    "pkgfiles": "egg-bin pkgfiles",
    "autod": "autod"
  },
  "files": [
    ".autod.conf.js",
    "config",
    "agent.js",
    "app.js",
    "lib"
  ],
  "ci": {
    "version": "6, 8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quinton/egg-mongoose-logger.git"
  },
  "bugs": {
    "url": "https://github.com/Quinton/egg/issues"
  },
  "homepage": "https://github.com/Quinton/egg-mongoose-logger#readme",
  "author": "quinton.xu@gmail.com",
  "license": "MIT",
  "main": ".autod.conf.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
